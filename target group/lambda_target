terraform {
  required_providers {
    aws = {
      source  = "hashicorp/aws"
      version = "~> 3.27"
    }
  }

  required_version = ">= 0.14.9"
}

provider "aws" {
  profile = "default"
  region  = "us-west-2"
}

resource "aws_lb_target_group" "lambda-target" {
  name        = "tf-example-lb-tg"
  target_type = "lambda"
}
